def solution(A):
    A.sort()  # Sort the list in ascending order
    max_count = 2  # At least two integers can always be chosen

    diff = A[1] - A[0]  # Calculate the initial difference

    count = 2  # Start with two integers chosen

    for i in range(2, len(A)):
        if A[i] - A[i - 1] == diff:
            count += 1
            max_count = max(max_count, count)
        else:
            diff = A[i] - A[i - 1]
            count = 2

    return max_count
solution(eval(input))
